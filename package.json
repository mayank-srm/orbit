{
  "name": "@appuo/orbit",
  "version": "1.0.10",
  "description": "Switch Vercel identities instantly. Multi-account profile manager for Vercel CLI. No logout required.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/appuo/orbit.git"
  },
  "type": "module",
  "bin": {
    "orbit": "dist/index.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "assets"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts",
    "dev": "tsup src/index.ts --format esm --dts --watch",
    "typecheck": "tsc --noEmit",
    "build:test": "tsc -p tsconfig.test.json",
    "test:run": "node --test --test-concurrency=1 tests/*.test.mjs",
    "test": "pnpm build && pnpm build:test && pnpm test:run",
    "test:coverage": "node --test --experimental-test-coverage --test-concurrency=1 tests/*.test.mjs",
    "changelog:add": "node scripts/changelog.mjs add",
    "changelog:release": "node scripts/changelog.mjs release",
    "release:prepare": "pnpm changelog:release && pnpm release:check",
    "verify:release": "node scripts/check-release.mjs",
    "smoke:package": "node scripts/smoke-package.mjs",
    "release:check": "pnpm typecheck && pnpm test && pnpm verify:release && pnpm smoke:package && npm --cache ./.npm-cache pack --dry-run",
    "release:publish": "pnpm release:check && npm --cache ./.npm-cache publish --registry=https://npm.pkg.github.com",
    "release:publish:npm": "pnpm release:check && npm --cache ./.npm-cache --@appuo:registry=https://registry.npmjs.org publish --access public --registry=https://registry.npmjs.org && npm --cache ./.npm-cache --@appuo:registry=https://registry.npmjs.org dist-tag add @appuo/orbit@$npm_package_version latest --registry=https://registry.npmjs.org",
    "release:ship": "node scripts/release.mjs",
    "prepublishOnly": "pnpm build"
  },
  "keywords": [
    "vercel",
    "vercel-switch-account",
    "vercel-multiple-accounts",
    "vercel-login",
    "vercel-profile-manager",
    "cloud-profiles",
    "multi-cloud",
    "cli",
    "credentials",
    "devtools",
    "aws-profiles",
    "orbit"
  ],
  "author": "Appuo",
  "license": "MIT",
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "chalk": "^5.6.2",
    "commander": "^14.0.3",
    "execa": "^9.6.1",
    "ora": "^9.3.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/node": "^25.2.3",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  }
}
